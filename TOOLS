public class Program
{

    static void Main()
    {
        //Console.SetIn(new StreamReader(Console.OpenStandardInput(),
        //                       Console.InputEncoding,
        //                       false,
        //                       bufferSize: 99999999));


       
    }

    public static int ConvertToInt(string s)
    {
        int y = 0;
        foreach (char c in s)
        {
            y = y * 10 + (c - '0');
        }
        return y;
    }
    
}
